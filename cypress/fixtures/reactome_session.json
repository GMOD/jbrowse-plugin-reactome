{
  "assemblies": [
    {
      "name": "volvox",
      "aliases": ["vvx"],
      "sequence": {
        "type": "ReferenceSequenceTrack",
        "trackId": "volvox_refseq",
        "adapter": {
          "type": "TwoBitAdapter",
          "twoBitLocation": {
            "uri": "test_data/volvox/volvox.2bit"
          }
        }
      },
      "refNameAliases": {
        "adapter": {
          "type": "FromConfigAdapter",
          "features": [
            {
              "refName": "ctgA",
              "uniqueId": "alias1",
              "aliases": ["A", "contigA"]
            },
            {
              "refName": "ctgB",
              "uniqueId": "alias2",
              "aliases": ["B", "contigB"]
            }
          ]
        }
      }
    }
  ],
  "plugins": [
    {
      "name": "Reactome",
      "url": "http://localhost:9200/dist/jbrowse-plugin-reactome.umd.development.js"
    }
  ],
  "defaultSession": {
    "name": "New session",
    "views": [
      {
        "type": "ReactomeView",
        "displayName": "Reactome View"
      }
    ]
  }
}
